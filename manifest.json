{
  "name": "Cognitofy",
  "version": "1.0",
  "description": "Open tab in Incognito",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {},
  "permissions": ["contextMenus", "activeTab"],
  //   "page_action": {
  //     "default_icon": {
  //       "16": "images/toolbar_icon16.png",
  //       "32": "images/toolbar_icon32.png",
  //       "48": "images/toolbar_icon48.png",
  //       "128": "images/toolbar_icon128.png"
  //     }
  //   },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "chromeos": "Alt+I",
        "linux": "Alt+I",
        "mac": "Alt+I",
        "windows": "Alt+I"
      }
    }
  }
}
